<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_include_profile_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".activites.MainActivity"
    >

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/activity_profile_toolbar"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="#BB86FC">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="30dp"
                android:text="My Profile"
                android:textColor="#FFFFFF"
                android:textSize="16sp"
                android:fontFamily="@font/poppins_regular"
                />
        </LinearLayout>

    </androidx.appcompat.widget.Toolbar>
    <LinearLayout
        android:id="@+id/activity_profile_llayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@color/white"
        android:layout_below="@id/activity_profile_toolbar">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="10dp"
            android:layout_weight="0.2">
            <TextView
                android:id="@+id/activity_profile_textView_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="name"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_regular"
                />
            <TextView
                android:id="@+id/activity_profile_textView_mobile_no"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:text="mobile neumber"
                android:fontFamily="@font/poppins_regular"
                />
            <TextView
                android:id="@+id/activity_profile_textView_email_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="email"
                android:textColor="@color/black"
                android:fontFamily="@font/poppins_regular"
                />
        </LinearLayout>
        <ImageView
            android:id="@+id/activity_profile_imgView_edit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.8"
            android:src="@drawable/ic_edit"
            android:layout_gravity="center|end"/>
    </LinearLayout>
    <View
        android:id="@+id/view1"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#F5F5F5"
        android:layout_below="@+id/activity_profile_llayout1"/>
    <LinearLayout
        android:id="@+id/activity_profile_llayout_helpCenter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view1"
        android:orientation="horizontal"
        android:padding="15dp"
        android:background="@color/white">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_location"
            android:layout_weight="0.9"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Help Center"
            android:textColor="@color/black"
            android:textSize="14sp"
            android:layout_weight="0.1"
            android:fontFamily="@font/poppins_regular"
            />

    </LinearLayout>

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#F5F5F5"
        android:layout_below="@+id/activity_profile_llayout_helpCenter"/>
    <LinearLayout
        android:id="@+id/activity_profile_llayout_manageAddress"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view2"
        android:orientation="horizontal"
        android:padding="15dp"
        android:background="@color/white">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_location"
            android:layout_weight="0.9"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=" Manage Address "
            android:textColor="@color/black"
            android:textSize="14sp"
            android:layout_weight="0.1"
            android:fontFamily="@font/poppins_regular"
            />
    </LinearLayout>
    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#F5F5F5"
        android:layout_below="@+id/activity_profile_llayout_manageAddress"/>
    <LinearLayout
        android:id="@+id/activity_profile_llayout_myBookings"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view3"
        android:orientation="horizontal"
        android:padding="15dp"
        android:background="@color/white">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_location"
            android:layout_weight="0.9"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=" My Bookings "
            android:textColor="@color/black"
            android:textSize="14sp"
            android:layout_weight="0.1"
            android:fontFamily="@font/poppins_regular"
            />
    </LinearLayout>
    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#F5F5F5"
        android:layout_below="@+id/activity_profile_llayout_myBookings"/>
    <LinearLayout
        android:id="@+id/activity_profile_llayout_registerAsPartner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view4"
        android:orientation="horizontal"
        android:padding="15dp"
        android:background="@color/white">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_location"
            android:layout_weight="0.9"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=" Register As Partner "
            android:textColor="@color/black"
            android:textSize="14sp"
            android:layout_weight="0.1"
            android:fontFamily="@font/poppins_regular"
            />
    </LinearLayout>
    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#F5F5F5"
        android:layout_below="@+id/activity_profile_llayout_registerAsPartner"/>
    <LinearLayout
        android:id="@+id/activity_profile_llayout_abourOurCompany"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view5"
        android:orientation="horizontal"
        android:padding="15dp"
        android:background="@color/white">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_location"
            android:layout_weight="0.9"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=" About Our Company "
            android:textColor="@color/black"
            android:textSize="14sp"
            android:layout_weight="0.1"
            android:fontFamily="@font/poppins_regular"
            />
    </LinearLayout>
    <View
        android:id="@+id/view6"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#F5F5F5"
        android:layout_below="@+id/activity_profile_llayout_abourOurCompany"/>
    <LinearLayout
        android:id="@+id/activity_profile_llayout_shareOurCompany"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view6"
        android:orientation="horizontal"
        android:padding="15dp"
        android:background="@color/white">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_location"
            android:layout_weight="0.9"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=" Share Our Company "
            android:textColor="@color/black"
            android:textSize="14sp"
            android:layout_weight="0.1"
            android:fontFamily="@font/poppins_regular"
            />
    </LinearLayout>
    <View
        android:id="@+id/view7"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#F5F5F5"
        android:layout_below="@+id/activity_profile_llayout_shareOurCompany"/>
    <LinearLayout
        android:id="@+id/activity_profile_llayout_settings"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view7"
        android:orientation="horizontal"
        android:padding="15dp"
        android:background="@color/white">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_location"
            android:layout_weight="0.9"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=" Settings "
            android:textColor="@color/black"
            android:textSize="14sp"
            android:layout_weight="0.1"
            android:fontFamily="@font/poppins_regular"
            />
    </LinearLayout>

    <TextView
        android:id="@+id/activity_profile_textView_logOut"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=" Logout "
        android:layout_below="@id/activity_profile_llayout_settings"
        android:layout_marginTop="80dp"
        android:gravity="center"
        android:textSize="14sp"
        android:textColor="#D6235F"
        android:textStyle="bold"
        android:fontFamily="@font/poppins_regular"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=" Version: 1.0.0 "
        android:textSize="12sp"
        android:layout_below="@id/activity_profile_textView_logOut"
        android:layout_marginTop="6dp"
        android:gravity="center"
        android:fontFamily="@font/poppins_regular"
        />


</RelativeLayout>